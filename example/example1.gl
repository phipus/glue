class Car {
    pub let brand: String;
    pub let model: String;
    var odo: float;

    pub new(brand: String, model: String) {
        this.brand = brand;
        this.model = model;
        this.odo = 0.0;
    }

    pub fn drive(dist: float) {
        odo += dist
    }

    pub fn getODO() float {
        return odo
    }
}

fn newCar(brand: String, model: String) Car {
    return Car {
        brand: brand,
        model: model,
    };
}